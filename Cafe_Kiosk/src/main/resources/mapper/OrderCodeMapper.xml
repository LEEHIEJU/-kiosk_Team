<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.hizju.cafekiosk.dao.OrdercodeDAO">
	<!-- namespace를 인터페이스 이름과 일치 시켜야 한다. -->
	<select id="date" resultType="string">
		select sysdate from dual
	</select>
	
	<!-- 주문화면 -->
	<select id="ordercodepage" parameterType="string" resultType="OrderCodeVO">
		select 
		ORDERNUM AS ordernum,
		ORDERWHERE AS orderwhere,
		ORDERIN AS orderin,
		sysdate AS ORDERDATE,
		COUPON AS coupon
		from ORDERCODE
	</select>
	
</mapper>